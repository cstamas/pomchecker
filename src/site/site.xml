<project name="PomChecker">
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.9</version>
    </skin>

    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <navBarStyle>navbar-inverse</navBarStyle>
            <sideBarEnabled>false</sideBarEnabled>
            <breadcrumbDivider>Â»</breadcrumbDivider>
            <gitHub>
                <projectId>kordamp/pomchecker</projectId>
                <ribbonOrientation>right</ribbonOrientation>
            </gitHub>
            <twitter>
                <user>aalmiray</user>
            </twitter>
        </fluidoSkin>
    </custom>

    <version position="right"/>
    <publishDate format="dd MMM yyyy" position="right"/>

    <body>
        <breadcrumbs>
            <item name="PomChecker" href="/"/>
        </breadcrumbs>

        <menu name="Overview" inherit="top">
            <item name="Introduction" href="index.html"/>
            <item name="License" href="http://www.apache.org/licenses/"/>
        </menu>

        <menu name="Additional information" inherit="top">
            <item name="Kordamp" href="https://kordamp.org"/>
        </menu>

        <menu ref="modules"/>

        <menu ref="reports" inherit="bottom"/>
    </body>
</project>
